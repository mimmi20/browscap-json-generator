{
  "name": "mimmi20/browscap-json-generator",
  "type": "application",
  "description": "Browser Capabilities Tools",
  "keywords": [
    "browscap"
  ],
  "homepage": "https://github.com/mimmi20/browscap-json-generator",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/browscap-json-generator/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.1",
    "beberlei/assert": "^3.2.7",
    "browscap/browscap": "^6.0.36",
    "browscap/browscap-php": "^3.1.0",
    "mimmi20/browscap-json-cache": "^1.0.4",
    "symfony/console": "^3.4.36 || ^4.4.3 || ^5.0.2",
    "thadafinser/package-info": "^1.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.2.2",
    "ergebnis/phpstan-rules": "^0.14.2",
    "friendsofphp/php-cs-fixer": "^2.16.1",
    "pepakriz/phpstan-exception-rules": "^0.10.1",
    "phpstan/phpstan": "^0.12.8",
    "phpstan/phpstan-deprecation-rules": "^0.12.2",
    "phpstan/phpstan-strict-rules": "^0.12.2",
    "squizlabs/php_codesniffer": "^3.5.3"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Browscap\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BrowscapTest\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "bin": [
    "bin/browscap-with-json"
  ],
  "support": {
    "issues": "https://github.com/mimmi20/browscap-json-generator/issues",
    "source": "https://github.com/mimmi20/browscap-json-generator"
  }
}
